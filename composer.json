{
    "name": "onestrive/heroicon",
    "description": "Fork version with updated dependencies: A Laravel Nova field for managing icons.",
    "keywords": [
        "laravel",
        "nova",
        "heroicons",
        "svg"
    ],
    "license": "MIT",
    "authors": [
      {
        "name": "Alex Azartsev",
        "email": "alex.azarsecond@gmail.com",
        "homepage": "https://github.com/OneStrive/heroicon",
        "role": "Original Developer"
      }
    ],
    "require": {
        "php": ">=8.1.0",
        "laravel/nova": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "OneStrive\\Heroicon\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OneStrive\\Heroicon\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
      {
        "type": "composer",
        "url": "https://nova.laravel.com"
      }
    ]
}
